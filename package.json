{
  "name": "gofriendcodes",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node ./server/app.js --proxy-config proxy.conf.json",
    "server": "nodemon ./server/app.js",
    "build": "ng build --prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "localhost": "ng build && node ./server/app.js",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor ./e2e/protractor.conf.js",
    "dev:ssr": "ng run gofriendcodes:serve-ssr ",
    "serve:ssr": "node dist/gofriendcodes/server/main.js",
    "build:ssr": "ng build && ng run gofriendcodes:server",
    "prerender": "ng run gofriendcodes:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "^13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/material": "^13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/platform-server": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@nguniversal/express-engine": "^13.0.2",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "http-errors": "^2.0.0",
    "jasmine": "^4.0.2",
    "moment": "^2.29.1",
    "mongoose": "^6.0.13",
    "morgan": "^1.10.0",
    "ngx-moment": "^5.0.0",
    "node": "^16.13.1",
    "nodemon": "^2.0.15",
    "protractor": "^7.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.1",
    "@angular/cli": "^13.1.1",
    "@angular/compiler-cli": "~13.1.0",
    "@nguniversal/builders": "^13.0.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
